Section4:AddButton({
	Name = "Kill All",
	Callback = function()
		if tonumber(game.Players.LocalPlayer.PlayerGui.Start.Powers.Burn.T.Text) >= tonumber(1) then
			local cf = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
			local ch = game.Players.LocalPlayer.Character.HumanoidRootPart
			game.Players.LocalPlayer.PlayerGui.Start.Powers.Burn.T.Text=game.Players.LocalPlayer.PlayerGui.Start.Powers.Burn.T.Text-1
			workspace.FE.Powers.Burn:FireServer()
			wait(0.2)
			for _,P in ipairs(game.Players:GetPlayers()) do
			wait(0.1)
			local cfps = P.Character.HumanoidRootPart.Position
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(cfps)
			print(P)
			end
			wait(1)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(cf)
		else
			OrionLib:MakeNotification({
				Name = "Error",
				Content = "You Do Not Have Enough Burn Power",
				Image = "rbxassetid://7734068321",
				Time = 5
			})
		end
  	end    
})
spawn(function()
Section4:AddSlider({
	Name = "Speed",
	Min = 0,
	Max = 10,
	Default = 0,
	Color = Color3.fromRGB(255,255,255),
	Increment = 0.1,
	ValueName = "",
	Callback = function(Value)
		function tpwalk(args)
			getgenv().tpwalking = false
			wait(0.2)
			getgenv().tpwalking = true
            while getgenv().tpwalking do
            pcall(function()
			local RunService = game:GetService("RunService")
			local hb = RunService.Heartbeat
			local chr = game.Players.LocalPlayer.Character
			local hum = chr and chr:FindFirstChildWhichIsA("Humanoid")
				local delta = hb:Wait()
				if hum.MoveDirection.Magnitude > 0 then
					if args == Value then
						chr:TranslateBy(hum.MoveDirection * tonumber(args) * delta * 10)
					else
						chr:TranslateBy(hum.MoveDirection * delta * 10)
					end
				end
            end)
			end
		end
		tpwalk(Value)
	end    
})
end)
Section2:AddSlider({
	Name = "FOV",
	Min = 70,
	Max = 120,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "",
	Callback = function(Value)
		game:GetService("Workspace").Camera.FieldOfView = Value
	end    
})
Section2:AddToggle({
	Name = "Ball Esp",
	Default = false,
	Callback = function(Value)
		getgenv().Esp = Value 
		if getgenv().Esp then
            pcall(function()
			game:GetService("Workspace").TPSSystem.TPS.Esp:Destroy()
			end)
			local Highlight = Instance.new("Highlight")
			Highlight.Name = 'Esp'
			Highlightcopy = Highlight:Clone()
			Highlight.Parent = game:GetService("Workspace").TPSSystem.TPS
		elseif getgenv().Esp==false then
			pcall(function()
			game:GetService("Workspace").TPSSystem.TPS.Esp:Destroy()
			end)
		end
	end    
})
