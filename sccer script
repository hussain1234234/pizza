Tab4:AddButton({
	Name = "CelebrationB",
	Callback = function()
      		if game.Players.LocalPlayer.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
				local player = game.Players.LocalPlayer
				local char = player.Character
				local hum = char.Humanoid
				local tt = game.Players.LocalPlayer.PlayerGui.Animations.NewCelebrationB
				local BirdAnimationTrack = hum:LoadAnimation(tt)
				BirdAnimationTrack:Play()
			else
				local player = game.Players.LocalPlayer
				local char = player.Character
				local hum = char.Humanoid
				local tt = game.Players.LocalPlayer.PlayerGui.Animations.CelebrationB
				local BirdAnimationTrack = hum:LoadAnimation(tt)
				BirdAnimationTrack:Play()
			end
  	end    
})
