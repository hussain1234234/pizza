-- Scripts:

local function DFPRQ_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	MainButton = script.Parent
	MainFrame = MainButton.Parent
	MainUi = MainFrame.Parent
	MainButton.MouseButton1Up:Connect(function()
		MainUi:Destroy()
	end)
end
coroutine.wrap(DFPRQ_fake_script)()
local function HXEJZVH_fake_script() -- PositionPrinter.LocalScript 
	local script = Instance.new('LocalScript', PositionPrinter)

	MainFrame = script.Parent.Parent
	localCh = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
	while true do 
		wait()
            pcall(function()
            if script.Parent.Parent.TextButton1.Text == "ON" then
            local x = tostring(math.round(localCh.Position.X))
            local y = tostring(math.round(localCh.Position.Y))
            local z = tostring(math.round(localCh.Position.Z))
            wait()
            script.Parent.Text =  tostring(x) ..",".. tostring(y) ..",".. tostring(z)
            end
        end)
	end
end
coroutine.wrap(HXEJZVH_fake_script)()
local function TDKIP_fake_script() -- TextButton1.LocalScript 
	local script = Instance.new('LocalScript', TextButton1)

	MainButton = script.Parent
	MainFrame = MainButton.Parent
	MainUi = MainFrame.Parent
	MainButton.MouseButton1Up:Connect(function()
		if MainButton.Text == "ON" then
			MainButton.Text = "OFF"
			MainButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
		elseif MainButton.Text ~= "ON" then
			MainButton.Text = "ON"
			MainButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
		end
	end)
end
coroutine.wrap(TDKIP_fake_script)()
local function FFYPR_fake_script() -- TextButton2.LocalScript 
	local script = Instance.new('LocalScript', TextButton2)

	local MainButton = script.Parent
	MainButton.MouseButton1Up:Connect(function()
		setclipboard(script.Parent.Parent["Position Printer"].Text)
	end)
end
coroutine.wrap(FFYPR_fake_script)()
local function VEYTSQX_fake_script() -- Position.LocalScript 
	local script = Instance.new('LocalScript', Position)

	frame = script.Parent.Frame
	frame.Draggable = true
	frame.Selectable = true
	frame.Active = true
	
end
coroutine.wrap(VEYTSQX_fake_script)()
end
