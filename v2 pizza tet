    game:GetService'Players'.LocalPlayer.Character['RightHand']:Destroy()

    game:GetService'Players'.LocalPlayer.Character['RightLowerArm']:Destroy()

    game:GetService'Players'.LocalPlayer.Character['RightUpperArm']:Destroy()

